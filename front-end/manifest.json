{
  	"manifest_version": 2,
  	"name": "FakeNewsGuard",
  	"version": "0.1",
  	"content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  	],
	"permissions": [
		"http://*/*",
        "https://*/*",
	  	"contextMenus", 
	  	"tabs",
	    "activeTab"],
  	"browser_action": {
     	"default_icon": "icon.png"
  	},
  	"background": {"scripts": ["main.js"]}
}
